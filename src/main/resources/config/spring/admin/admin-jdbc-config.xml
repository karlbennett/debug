<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation=" http://www.springframework.org/schema/beans
                            http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
                            http://www.springframework.org/schema/context
                            http://www.springframework.org/schema/context/spring-context-3.0.xsd">

    <import resource="classpath:config/spring/db-util-config.xml"/>
    <import resource="classpath:config/spring/admin/admin-data-source-config.xml"/>

    <bean name="adminJdbcTemplate" class="org.springframework.jdbc.core.JdbcTemplate">
        <constructor-arg ref="adminDataSource"/>
    </bean>

    <context:annotation-config/>
    <bean id="adminJdbcDaoImpl" class="org.youthnet.debug.dao.jdbc.impl.AdminJdbcDaoImpl"/>
</beans>