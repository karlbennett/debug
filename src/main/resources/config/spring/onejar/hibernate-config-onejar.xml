<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation=" http://www.springframework.org/schema/beans
                            http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
                            http://www.springframework.org/schema/context
                            http://www.springframework.org/schema/context/spring-context-3.0.xsd">

    <import resource="classpath:config/spring/onejar/admin-hibernate-config-onejar.xml"/>
    <import resource="classpath:config/spring/onejar/core-hibernate-config-onejar.xml"/>

    <!-- Explicitly list required beans so that they are seen within the onejar.  -->
    <context:annotation-config/>
    <bean id="testTableDao" class="org.youthnet.debug.DebugClient"/>

</beans>